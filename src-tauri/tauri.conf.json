{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Extraction point",
  "version": "0.1.0",
  "identifier": "com.extraction.point",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Extraction point",
        "width": 1460,
        "height": 844,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.ico"]
  }
}
